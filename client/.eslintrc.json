{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "eslint-config-airbnb"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "react/prop-types": "off",
    "react/jsx-props-no-spreading": "off"
  },
  "settings": {
    "import/ignore": [".css$"]
  }
}
